{
	"name": "vite-react-ssr",
	"description": "React SSR powered by Vite & fastify",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "node vitify/bin.mjs",
		"build": "concurrently -n w: yarn:build:*",
		"build:client": "vite build --outDir dist/client",
		"build:server": "cross-env BUILD_ENV=SSR vite build --ssr src/server/prod.ts --outDir dist/server",
		"serve": "cross-env NODE_ENV=production node dist/server",
		"debug": "cross-env NODE_ENV=development node --inspect-brk dist/server"
	},
	"dependencies": {
		"cross-fetch": "^3.1.5",
		"dotenv-flow": "^3.2.0",
		"fastify": "^3.27.0",
		"fastify-compress": "^4.0.1",
		"fastify-cookie": "^5.5.0",
		"fastify-fetch": "^1.0.3",
		"fastify-plugin": "^3.0.0",
		"fastify-static": "^4.5.0",
		"is-browser": "^2.1.0",
		"middie": "^6.0.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-helmet-async": "^1.2.2",
		"react-query": "^3.34.12",
		"react-router": "^5.2.1",
		"react-router-config": "^5.1.1",
		"react-router-dom": "^5.3.0",
		"serialize-error": "^9.0.0",
		"serialize-javascript": "^6.0.0"
	},
	"devDependencies": {
		"@types/dotenv-flow": "^3.2.0",
		"@types/node": "^16.11.21",
		"@types/react": "^17.0.37",
		"@types/react-dom": "^17.0.11",
		"@types/react-router": "^5.1.17",
		"@types/react-router-config": "^5.0.6",
		"@types/react-router-dom": "^5.3.3",
		"@types/serialize-javascript": "^5.0.2",
		"@vitejs/plugin-react": "^1.1.4",
		"concurrently": "^7.0.0",
		"cross-env": "^7.0.3",
		"typescript": "^4.5.5",
		"vite": "^2.7.13",
		"vite-node": "^0.2.3",
		"vite-plugin-node": "^0.0.15",
		"vite-plugin-pages": "~0.18.2",
		"vite-tsconfig-paths": "^3.3.17"
	}
}
